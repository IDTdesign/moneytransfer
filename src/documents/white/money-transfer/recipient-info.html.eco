<div><img src="<%= @getUrl('/img-white/recipient-info-form.png') %>" alt=""></div>
<div><img src="<%= @getUrl('/img-white/address-form.png') %>" alt=""></div>
<div>
    <a id="checkPath2" href="#"
        data-credit="<%= @getUrl('/white/money-transfer/credit-card-info') %>"
        data-debit="<%= @getUrl('/white/money-transfer/credit-card-info') %>"
        data-withdrawal="<%= @getUrl('/white/money-transfer/withdrawal') %>"
        ><img src="<%= @getUrl('/img-white/continue.png') %>" alt=""></a>
</div>
