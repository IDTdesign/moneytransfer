            <img class="pickup-credit pickup-debit pickup-withdrawal" src="<%= @getUrl('/img-white/dynamic/condition-pickup.png') %>" alt="">
            <img class="bank-credit bank-debit bank-withdrawal" src="<%= @getUrl('/img-white/dynamic/condition-bank.png') %>" alt="">
            <img class="home-credit home-debit home-withdrawal" src="<%= @getUrl('/img-white/dynamic/condition-home.png') %>" alt="">

            <img class="pickup-credit bank-credit home-credit" src="<%= @getUrl('/img-white/dynamic/condition-credit.png') %>" alt="">
            <img class="pickup-debit bank-debit home-debit" src="<%= @getUrl('/img-white/dynamic/condition-debit.png') %>" alt="">
